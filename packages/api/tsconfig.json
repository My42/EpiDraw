{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "paths": {
      "@interfaces/*": ["./src/interfaces/*"],
      "@models/*": ["./src/models/*"],
      "myTypes/*": ["./src/types/*"],
      "@services/*": ["./src/services/*"],
      "@shared/*": ["../shared/*"],
      "test/*": ["./test/*"]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
